{
  "name": "adonis-boilerplate",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "test": "node ace test",
    "test.old": "echo 'Error: no test specified' && exit 1",
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json srivadero/adonis-boilerplate",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@symfony/webpack-encore": "^3.0.0",
    "@types/jsdom": "^16.2.15",
    "adonis-preset-ts": "^2.1.0",
    "alpinejs": "^3.10.3",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^2.20.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "mrm": "^4.0.0",
    "np": "^7.6.2",
    "pino-pretty": "^8.1.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "ts-loader": "^9.3.1",
    "typescript": "~4.7",
    "unpoly": "^2.6.1",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.1",
    "@adonisjs/core": "^5.8.5",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.0.11",
    "@adonisjs/view": "^6.1.6",
    "@japa/api-client": "^1.4.0",
    "better-sqlite3": "^7.6.2",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "16.13.1",
    "probotApps": [],
    "runGhActionsOnWindows": false
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "license": "MIT",
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
